{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "checkout_session": {
      "type": "string"
    },
    "tournament": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament.tournament",
      "inversedBy": "orders"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "camp": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::camp.camp",
      "inversedBy": "orders"
    }
  }
}
